{
  "_version": "1.1.0",
  "sap.app": {
    "_version": "1.1.0",
    "id": "test",
    "type": "application",
    "i18n": "resources/i18n/texts.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "ach": "CA-UI5-DOC"
  },
  "sap.ui": {
    "_version": "1.1.0",
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "fullWidth": false,
    "supportedThemes": ["sap_belize_plus", "sap_belize", "sap_bluecrystal"]
  },
  "sap.ui5": {
    "_version": "1.1.0",
    "dependencies": {
      "minUI5Version": "1.44",
      "libs": {
        "sap.m": {},
        "sap.suite.ui.commons": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "resources/i18n/texts.properties"
      },
      "data": {
        "type": "sap.ui.model.odata.v4.ODataModel",
        "uri": "/sap/opu/odata/my/data/",
        "settings": {
          "groupId": "$direct",
          "synchronizationMode": "None"
        }
      }
    }
  }
}
